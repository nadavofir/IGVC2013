<launch>

  <arg name="debug" default="false"/>

  <node pkg="image_view" type="image_view" name="dbg_view" output="screen">
    <remap from="image" to="/gests/dbgimg"/>
  </node>

  <node pkg="image_view" type="image_view" name="image_view" output="screen">
    <remap from="image" to="/gests/dptimg"/>
  </node>

  <node pkg="dynamic_reconfigure" type="reconfigure_gui" name="reconfigure_gui"/>

  <include file="$(find gests)/launch/gests.launch">
    <arg name="debug" value="$(arg debug)"/>
  </include>

</launch>
